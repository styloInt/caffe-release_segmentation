layer {
  name: "deconv4"
  type: "Deconvolution"
  bottom: "pool3"
  top: "deconv4"
  param {
    lr_mult: 1
  }
  param {
    lr_mult: 2
  }
  convolution_param {
    num_output: 32
    #bias_term: false
    engine: CAFFE
    kernel_size: 4
    pad: 1
    stride: 2
    weight_filler {
      type: "xavier"
      #std: 0.01
    }
  }
}
layer {
  name: "concat4"
  type: "Concat" 
  bottom: "conv3ao"
  bottom: "deconv4"
  top: "deconv4ao"
  #concat_param {
  #concat_dim:1
  #}
}
layer {
  name: "relu4"
  type: "ReLU"
  bottom: "deconv4ao"
  top: "deconv4r"
}
#------------layer group 5-------------
layer {
  name: "deconv5"
  type: "Deconvolution"
  bottom: "deconv4r"
  top: "deconv5"
  param {
    lr_mult: 1
  }
  param {
    lr_mult: 2
  }
  convolution_param {
    num_output: 32
    #bias_term: false
    engine: CAFFE
    kernel_size: 4
    pad: 1
    stride: 2
    weight_filler {
      type: "xavier"
      #std: 0.01
    }
  }
}
layer {
  name: "concat5"
  type: "Concat" 
  bottom: "conv2bo"
  bottom: "deconv5"
  top: "deconv5ao"
  concat_param {
    concat_dim:1
  }
}
layer {
  name: "relu5"
  type: "ReLU"
  bottom: "deconv5ao"
  top: "score"
}
#------------layer group 6-------------
layer {
  name: "deconv6"
  type: "Deconvolution"
  bottom: "score"
  top: "deconv6"
  param {
    lr_mult: 1
  }
  param {
    lr_mult: 2
  }
  convolution_param {
    num_output: 32
    #bias_term: false
    engine: CAFFE
    kernel_size: 4
    pad: 1
    stride: 2
    weight_filler {
      type: "xavier"
      #std: 0.01
    }
  }
}
layer {
  name: "concat5"
  type: "Concat" 
  bottom: "conv1co"
  bottom: "deconv6"
  top: "deconv6ao"
  #concat_param {
  #concat_dim:1
  #}
}
layer {
  name: "relu6"
  type: "ReLU"
  bottom: "deconv6ao"
  top: "deconv6r"
}
layer {
  name: "convX"
  type: "Convolution"
  bottom: "deconv6r"
  top: "score-final"
  param {
    lr_mult: 1
  }
  param {
    lr_mult: 2
  }
  convolution_param {
    num_output: 3
    kernel_size: 3
    pad: 1
    stride: 1
    engine: CAFFE
    weight_filler {
      type: "xavier"
      #std: 0.01
    }
    bias_filler {
      type: "constant"
      value: 0
    }
  }
}
layer {
  name: "loss"
  type: "SoftmaxWithLoss"
  bottom: "score-final"
  bottom: "label"
  top: "loss"
    loss_param {
      normalize: false
    }
#  softmax_param {
#    axis: 1
#  }
  include: { phase: TRAIN }
}
layer {
  name: "loss"
  type: "SoftmaxWithLoss"
  bottom: "score-final"
  bottom: "label"
  top: "loss"
    loss_param {
      normalize: false
    }
#  softmax_param {
#    axis: 1
#  }
  include: { phase: TEST }
}

